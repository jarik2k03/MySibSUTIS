#!/usr/bin/gnuplot
set termoption enhanced
set terminal svg size 800,600 font "Arial, 16"
set output "results/plotD.svg"
set style line 1 lc rgb "0xDC143C" lt 1 lw 4 pt 9 ps 0.1

set border lw 2
set grid
set key top left

set xlabel "Кол-во дней исследования"
set ylabel "Кол-во испытуемых" rotate by 90

set xtics 10
set ytics 10
set mxtics

set format x "%g"
set format y "%.0f"

plot "data.dat" using 1:6 title "D - умершие" with linespoints ls 1